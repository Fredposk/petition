<h1>Results for: {{location}}</h1>



<table class="bg-gray-200">
    <tr>
        <td>First Name</td>
        <td>Last Name</td>
        <td>Age</td>
    </tr>
{{#each result}}
    <tr>
        <td>{{first}}</td>
        <td>{{last}}</td>
        <td>{{age}}</td>
    </tr>
{{/each}}
</table>

<a href="/signers" class="px-2 py-1 mt-3 text-sm tracking-wide text-gray-300 uppercase transition duration-500 bg-gray-500 border-2 border-gray-500 rounded shadow hover:bg-gray-700 hover:shadow-2xl hover:text-gray-400"
>Back</a>



{{#if hasDBErrors}}
</div>
 <div class="flex flex-col items-center justify-center mt-12">
  <ul class="errorEl">
    <li>{{errors}}</li>
{{/if}}
  </ul>
</div>


